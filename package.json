  "author": "José Miguel Sánchez Alés <sio2sio2+github@gmail.com>",
  "version": "0.2.2",
  "main": "dist/core.js",
  "scripts": {
    "build": "webpack --env.output=min --progress",
    "build:bundle": "webpack --env.output=bundle --progress",
    "build:src": "webpack --env.output=src --progress",
    "build:debug": "webpack --env.output=srcdebug --progress",
    "debug": "webpack-dev-server --env.output=debug --progress"
  },
  "license": "MIT",
  "keywords": [
    "gis",
    "map",
    "andalucia",
    "ofertaeducativa",
    "adjudicaciones",
    "educacion"
  ],
  "browserslist": {
    "production": [
      "cover 95% in ES"
    ],
    "oneplus": [
      "> 1% in ES"
    ],
    "ie10": [
      "> .25% in ES",
      "ie >= 10"
    ],
    "relax": [
      "cover 80% in ES"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sio2sio2/lobaton-core.git"
  },
  "homepage": "https://github.com/sio2sio2/lobaton-core#readme",
  "bugs": {
    "url": "https://github.com/sio2sio2/lobaton-core/issues"
  },
  "devDependencies": {
