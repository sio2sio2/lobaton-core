    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@turf/boolean-point-in-polygon": "^6.0.1",
    "@turf/difference": "^6.0.2",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
